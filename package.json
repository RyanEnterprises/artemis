{
  "name": "artemis",
  "productName": "artemis",
  "version": "5.0.0",
  "description": "an AI that adapts to you",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "build": "npx tsc src/ts/main.ts --outDir src/js && npx tsc src/ts/artemis.ts --outDir src/js",
    "buildStart": "npx tsc src/ts/main.ts --outDir src/js && npm run start && npx tsc src/ts/artemis.ts --outDir src/js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "hiteacheryouare",
    "email": "82683251+hiteacheryouare@users.noreply.github.com"
  },
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "artemis"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "maintainer": "Ryan Enterprises",
            "homepage": "https://github.com/RyanEnterprises/artemis"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "about-window": "^1.15.2",
    "demotivator": "^7.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "18.0.1"
  }
}
